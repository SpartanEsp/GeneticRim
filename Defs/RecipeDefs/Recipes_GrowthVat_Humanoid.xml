	<RecipeDef>
		<defName>GR_CraftBearClaws</defName>
		<label>Human Implant: bear claws</label>
		<description>Craft bear claws, a hybrid implant containing humanoid and bear DNA that adds a powerful melee attack.</description>
		<jobString>Crafting Bear Claws</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<workAmount>500</workAmount>
		<recipeUsers>
			<li>GR_TissueGrowingVat</li>	  
		</recipeUsers>	
		<ingredients>

			<li>
				<filter>
					<thingDefs>
						<li>GR_ImplantPlasteelWireMesh</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>GR_HumanoidGenetic</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>GR_BearGenetic</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>

		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>GR_ImplantPlasteelWireMesh</li>
				<li>GR_HumanoidGenetic</li>
				<li>GR_BearGenetic</li>
			</thingDefs>
		</fixedIngredientFilter>

		<products>
			<GR_BearClaws>1</GR_BearClaws>
		</products>
	</RecipeDef>
	
